    {
      "path": "dist/theme.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.5",
    "husky": "^7.0.4",
    "size-limit": "^7.0.5",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
    "dependencies": {
        "style-dictionary": "^3.0.2",
        "@theme-ui/css": "^0.13.1",
        "@theme-ui/preset-base": "^0.13.1",
        "deepmerge": "^4.2.2"
    }
}
